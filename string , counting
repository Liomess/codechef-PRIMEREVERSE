for i in range(int(input())):
    s=input()
    n=int(s)
    a=input()
    b=input()
    if a==b:
        print("yes")
    else:
        da,db=0,0
        for i in range(n):
            if a[i]=="1":
                da+=1
            if b[i]=="1":
                db+=1
        if da!=db:
            print("no")
        else:
            print("yes")
